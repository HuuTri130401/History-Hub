@page
@model HistoryHub.Pages.EditorAccess.Feedbacks.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Feedback[0].Comment)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Feedback[0].Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Feedback[0].CreateDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Feedback[0].Quiz)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Feedback[0].Timeline)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Feedback[0].User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Feedback) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Comment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quiz.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Timeline.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.Email)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.FeedbackId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.FeedbackId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.FeedbackId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
